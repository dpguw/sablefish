citHeader("To cite package 'FSA' in publications use:")

desc <- packageDescription("FSA")
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", desc$Date)
vers <- paste("R package version", desc$Version)
citEntry(entry="Manual",
         title = "FSA: Fisheries Stock Analysis",
         author = personList(as.person("Derek H. Ogle")),
         year = year,
         note = vers,
         textVersion = paste("Ogle, D.H. ",year,". FSA: Fisheries Stock Analysis. ",vers,".", sep="")
)
